/**index.wxss**/
.main{font-size:32rpx;font-family:'Microsoft YaHei';color:#555}
.container {
  padding-top: 30px;
}
.rectangle{
  width:600px;
  height:180px;
  background: #93c0e7;
}

.strengthlist{width:80%;height:140rpx;padding:5%;border-bottom:5px solid #eee;display: float}
.strengthimg{float:left;width:27%;height:100%;}
.strengthimg image{float:left;width:100%;height:100%;border-radius:15rpx;}
.strengthinfo{float:left;width:66%;text-align:center;line-height:60rpx;margin-left:3%}
.strengthinfo .ilist{clear:both;}


view.text2 {
  text-indent:40rpx;
  align-items: center;
  position: relative;
  padding: 10px 10px 10px 12px;
  font-size: 30rpx;
  line-height:50rpx;
  color: #656565;
}
view.text3 {
  width:92%;
  margin:auto;
  line-height:50rpx;
  align-items: center;
  position: relative;
  font-size: 30rpx;
  color: #656565;
}
.qrcode{width:100%;margin:30rpx auto}

.telicon{width:120rpx;height:120rpx;position:fixed;bottom:80rpx;right:20rpx;}
.telicon image{width:120rpx;height:120rpx;}